{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "Kubis1982": {
    "CORS": {
      "AllowCredentials": true,
      "AllowedMethods": [
        "POST",
        "PATCH",
        "PUT",
        "DELETE",
        "GET"
      ],
      "AllowedHeaders": [
        "Content-Type",
        "Authorization",
        "Accept"
      ],
      "ExposedHeaders": [
        "*"
      ]
    }
  }
}